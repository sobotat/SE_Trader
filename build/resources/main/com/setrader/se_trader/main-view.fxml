<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="440.0" prefWidth="915.0" spacing="20.0" stylesheets="@main-view.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.setrader.se_trader.Controller">

    <padding>
        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
    </padding>
    <HBox prefHeight="420.0" prefWidth="997.0">
        <VBox prefHeight="420.0" prefWidth="707.0">
            <HBox prefHeight="58.0" prefWidth="680.0">
                <ImageView fitHeight="48.0" fitWidth="462.0" pickOnBounds="true" preserveRatio="true">
                    <Image url="@../../../res/se_logo.png"/>
                </ImageView>
            </HBox>
            <TableView fx:id="gpsTable" prefHeight="306.0" prefWidth="470.0">
                <columns>
                    <TableColumn fx:id="nameCol" prefWidth="271.20001220703125" style="-fx-text: white;"
                                 text="Name"/>
                    <TableColumn minWidth="0.0" prefWidth="100.0" text="X" fx:id="xCol"/>
                    <TableColumn fx:id="yCol" minWidth="0.0" prefWidth="100.0" text="Y"/>
                    <TableColumn fx:id="zCol" minWidth="0.0" prefWidth="100.0" text="Z"/>
                    <TableColumn fx:id="colorCol" prefWidth="107.20001220703125" text="Color"/>
                </columns>
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                </columnResizePolicy>
                <VBox.margin>
                    <Insets top="25.0"/>
                </VBox.margin>
            </TableView>
            <HBox alignment="CENTER" prefHeight="53.0" prefWidth="470.0"/>
            <TextField fx:id="tf_GPS" prefHeight="25.0" prefWidth="518.0" promptText="Write GPS">
                <VBox.margin>
                    <Insets bottom="15.0"/>
                </VBox.margin>
            </TextField>
        </VBox>
        <VBox prefHeight="420.0" prefWidth="361.0">
            <VBox alignment="BOTTOM_CENTER" prefHeight="421.0" prefWidth="582.0">
                <VBox.margin>
                    <Insets left="25.0" right="15.0"/>
                </VBox.margin>
                <Button fx:id="btn_Load" onAction="#onLoadButtonClick" prefHeight="10.0" prefWidth="225.0"
                        text="Load">
                    <VBox.margin>
                        <Insets top="10.0"/>
                    </VBox.margin>
                </Button>
                <Button fx:id="btn_Save" onAction="#onSaveButtonClick" prefHeight="10.0" prefWidth="225.0"
                        text="Save">
                    <VBox.margin>
                        <Insets top="10.0"/>
                    </VBox.margin>
                </Button>
                <Button fx:id="btn_CalculateNext" onAction="#onCalculateNextButtonClick" prefHeight="10.0"
                        prefWidth="225.0" text="Calculate to Next">
                    <VBox.margin>
                        <Insets top="15.0"/>
                    </VBox.margin>
                </Button>
                <Button fx:id="btn_CalculateEntire" onAction="#onCalculateEntireButtonClick" prefHeight="10.0"
                        prefWidth="225.0" text="Calculate Entire">
                    <VBox.margin>
                        <Insets top="10.0"/>
                    </VBox.margin>
                </Button>
                <Button fx:id="btn_Enter" onAction="#onEnterButtonClick" prefHeight="10.0" prefWidth="225.0"
                        text="Enter">
                    <VBox.margin>
                        <Insets top="15.0"/>
                    </VBox.margin>
                </Button>
                <Button fx:id="btn_Remove" onAction="#onRemoveButtonClick" prefHeight="10.0" prefWidth="225.0"
                        text="Remove">
                    <VBox.margin>
                        <Insets top="10.0"/>
                    </VBox.margin>
                </Button>
                <Button fx:id="btn_View" onAction="#onViewButtonClick" prefHeight="10.0" prefWidth="225.0"
                        text="View">
                    <VBox.margin>
                        <Insets top="10.0"/>
                    </VBox.margin>
                </Button>
                <CheckBox fx:id="cb_BackHome" mnemonicParsing="false" prefHeight="18.0" prefWidth="91.0"
                          text="Back Home" textFill="WHITE">
                    <VBox.margin>
                        <Insets top="10.0"/>
                    </VBox.margin>
                </CheckBox>
                <ProgressBar fx:id="pb_status" prefHeight="25.0" prefWidth="225.0" progress="0.0">
                    <VBox.margin>
                        <Insets bottom="15.0" top="25.0"/>
                    </VBox.margin>
                </ProgressBar>
            </VBox>
        </VBox>
    </HBox>
</VBox>
