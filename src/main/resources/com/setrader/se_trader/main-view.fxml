<VBox prefHeight="440.0" prefWidth="900.0" spacing="20.0" stylesheets="@main-view.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.setrader.se_trader.Controller">
